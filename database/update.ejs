<html>

    <head>
        <title>Update</title>
		<meta charset="utf-8">
    </head>
    <body>
        
        <form method='post'>
            
            <table>
                
                <tr>
                    <td><label>ID</label></td>
                    <td><input type='text' name='id' value='<%= item.id %>' disabled='true'></td>
                </tr>
                
                <tr>
                    <td><label>Url</label></td>
                    <td><input type='text' name='url' value='<%= item.url %>'></td>
                </tr>
                
                <tr>
                    <td><label>Urlname</label></td>
                    <td><textarea name='urlname'><%= item.urlname %></textarea></td>
                </tr>
                
            </table>
            
            <input type='submit'>
            
        </form>
        
    </body>

</html>